    function rb()
         while _G.rb == true do
           game:GetService("ReplicatedStorage").GameClient.Events.RemoteEvent.UpdateRebirth:FireServer()
            wait(0.1)
        end 
    end
    
    function win()
         while _G.win == true do
         game:GetService("ReplicatedStorage"):WaitForChild("GameClient"):WaitForChild("Events"):WaitForChild("RemoteEvent"):WaitForChild("RestTimerDaliyChest"):FireServer(game:GetService("Players").LocalPlayer)
         wait(0)
        end
    end
    
